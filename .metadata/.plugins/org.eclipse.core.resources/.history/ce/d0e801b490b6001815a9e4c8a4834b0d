<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<script>
$(document).ready(function() {
	$.ajax({
		type: 'GET',
		url: 'http://www.kobis.or.kr/kobisopenapi/webservice/rest/boxoffice/searchWeeklyBoxOfficeList.json?key=a19c59ff51a6a8f37d37e30cd7e64bb9&targetDt=20180909',
		contentType: 'application/x-www-form-urlencoded; charset=utf-8',
		success: function(result) {
			var res = [];
			for(var i=0; i<3; i++) {
				res.push('<div class="col card mb-3">');
				res.push('<h4 class="card-header"><span class="badge badge-pill badge-info">'+result['boxOfficeResult']['weeklyBoxOfficeList'][i]['rank']+'ìœ„</span>'+result['boxOfficeResult']['weeklyBoxOfficeList'][i]['movieNm']+'</h4>');
				res.push('</div>')
			}
			
			$('#row').html(res.join(''));
		}
	});
});
</script>